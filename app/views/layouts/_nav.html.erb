<!-- We will render a specific nav layout dpending on the user is signed in or not -->

<% if !user_signed_in? %>
    <%= render '/layouts/nav_guest'%>
<% else%>
    <% if current_user.has_role? :admin %>
        <%= render '/layouts/nav_admin'%>
    <% else %>
        <%= render '/layouts/nav_user'%>
    <% end %>
<%end%>